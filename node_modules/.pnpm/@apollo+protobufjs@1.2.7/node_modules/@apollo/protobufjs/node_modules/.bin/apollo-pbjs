#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/taryd/OneDrive/Documents/BYU-I/Fall_2025/Web_Services/BuildOps/node_modules/.pnpm/@apollo+protobufjs@1.2.7/node_modules/@apollo/protobufjs/bin/node_modules:/mnt/c/Users/taryd/OneDrive/Documents/BYU-I/Fall_2025/Web_Services/BuildOps/node_modules/.pnpm/@apollo+protobufjs@1.2.7/node_modules/@apollo/protobufjs/node_modules:/mnt/c/Users/taryd/OneDrive/Documents/BYU-I/Fall_2025/Web_Services/BuildOps/node_modules/.pnpm/@apollo+protobufjs@1.2.7/node_modules/@apollo/node_modules:/mnt/c/Users/taryd/OneDrive/Documents/BYU-I/Fall_2025/Web_Services/BuildOps/node_modules/.pnpm/@apollo+protobufjs@1.2.7/node_modules:/mnt/c/Users/taryd/OneDrive/Documents/BYU-I/Fall_2025/Web_Services/BuildOps/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/taryd/OneDrive/Documents/BYU-I/Fall_2025/Web_Services/BuildOps/node_modules/.pnpm/@apollo+protobufjs@1.2.7/node_modules/@apollo/protobufjs/bin/node_modules:/mnt/c/Users/taryd/OneDrive/Documents/BYU-I/Fall_2025/Web_Services/BuildOps/node_modules/.pnpm/@apollo+protobufjs@1.2.7/node_modules/@apollo/protobufjs/node_modules:/mnt/c/Users/taryd/OneDrive/Documents/BYU-I/Fall_2025/Web_Services/BuildOps/node_modules/.pnpm/@apollo+protobufjs@1.2.7/node_modules/@apollo/node_modules:/mnt/c/Users/taryd/OneDrive/Documents/BYU-I/Fall_2025/Web_Services/BuildOps/node_modules/.pnpm/@apollo+protobufjs@1.2.7/node_modules:/mnt/c/Users/taryd/OneDrive/Documents/BYU-I/Fall_2025/Web_Services/BuildOps/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/pbjs" "$@"
else
  exec node  "$basedir/../../bin/pbjs" "$@"
fi
